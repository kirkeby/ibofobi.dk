<?xml version='1.0' encoding='utf-8'?>
<?xml-stylesheet href="http://media.ibofobi.dk/atom.css" type="text/css"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang='en'>
    <id>http://ibofobi.dk/blog/</id>
    <title>about:me</title>
    <link rel='self' type='application/atom+xml' href='http://ibofobi.dk/blog/feeds/latest/'/>
    <link rel='alternate' type='text/html' href='http://ibofobi.dk/blog/'/>
    <author><name>Sune Kirkeby</name></author>
    <updated tal:content='python:posts[0]["stamp"].strftime("%Y-%m-%dT%H:%M:%SZ")'/>

    <entry tal:repeat='post posts'>
        <id tal:content='post/tag'/>
        <published tal:content='python:post["stamp"].strftime("%Y-%m-%dT%H:%M:%SZ")'/>
        <updated tal:content='python:post["stamp"].strftime("%Y-%m-%dT%H:%M:%SZ")'/>
        <link rel='alternate' type='text/html'
              tal:attributes='href string:http://ibofobi.dk/blog/archive/${post/name}/'/>
        <title type='xhtml'><div xmlns='http://www.w3.org/1999/xhtml'
               tal:content='post/title'/>
        </title>
        <content type='xhtml'><div xmlns='http://www.w3.org/1999/xhtml'
                 tal:content='structure post/html'/>
        </content>
    </entry>
</feed>
